{
  "name": "area-codes-nanp",
  "version": "0.0.5",
  "description": "List of in-service (active and valid) U.S. area codes (NPAs) according to the North American Numbering Plan Administrator (NANPA)",
  "author": "Tony Pagaduan <Elte156@gmail.com>",
  "scripts": {
    "build": "webpack",
    "prebuild": "tsc",
    "test": "jest",
    "fetch-area-codes": "ts-node ./scripts/area_code_generator.ts",
    "format": "prettier --single-quote --write '**/(*.md|*.ts|*.yml|*.yaml|*.json)'",
    "format:check": "prettier --single-quote --check '**/(*.md|*.ts|*.yml|*.yaml|*.json)'",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.3",
    "csv-parser": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/typings/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "keywords": [
    "area",
    "codes",
    "phone",
    "number",
    "NPA",
    "nanp",
    "NANPA"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Elte156/area-codes-nanp.git"
  },
  "bugs": {
    "url": "https://github.com/Elte156/area-codes-nanp/issues"
  },
  "homepage": "https://github.com/Elte156/area-codes-nanp#readme",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  }
}
